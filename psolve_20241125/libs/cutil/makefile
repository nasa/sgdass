SHELL = /bin/csh -f -c 'umask 0002 ; eval "$2"'

LIB     = cutil.a
NUL     = /null/dev

.c.o:
	$(MK5_C) -c -o $*.o $*.c

.f.o:
	$(MK5_F95)     -c -o $*.o     $*.f

.f.opt_o:
	$(MK5_F95_OPT) -c -o $*.opt_o $*.f

.f.Opt_o:
	$(MK5_F95_OPTEST) -c -o $*.Opt_o $*.f  

.f.d:
	$(MK5_F95) -c -o $*.o $*.f

